{
	"name": "ssr",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"ng": "ng",
		"start": "ng serve -o --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check",
		"start:noproxy": "ng serve -o --host 0.0.0.0 --disable-host-check",
		"proxy": "ng serve -o --proxy-config proxy.conf.json",
		"build": "ng build",
		"build:stats": "ng build --stats-json",
		"build:test": "npx tsc -p tsconfig.node.json && sh copy_test_files.sh",
		"build:test:run": "npm run build:test && npm run test:p64",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"test:p64": "node dist/test/sendPdfAndXml.test.js",
		"test:v": "node dist/test/validator.js",
		"serve:ssr:ssr": "node dist/ssr/server/server.mjs",
		"ssr": "node dist/ssr/server/server.mjs",
		"serve:ssr:main": "node dist/ssr/server/main.server.mjs",
		"serve:watch": "nodemon",
		"server": "npm run build && npm run serve:ssr:ssr",
		"main:server": "npm run build && npm run serve:ssr:main"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.0.3",
		"@angular/common": "^18.0.3",
		"@angular/compiler": "^18.0.3",
		"@angular/core": "^18.0.3",
		"@angular/forms": "^18.0.3",
		"@angular/platform-browser": "^18.0.3",
		"@angular/platform-browser-dynamic": "^18.0.3",
		"@angular/platform-server": "^18.0.3",
		"@angular/router": "^18.0.3",
		"@angular/ssr": "^18.0.4",
		"axios": "^1.7.2",
		"body-parser": "^1.20.2",
		"chart.js": "^4.4.4",
		"express": "^4.18.2",
		"html2pdf.js": "^0.10.2",
		"multer": "^1.4.5-lts.1",
		"nocache": "^4.0.0",
		"primeflex": "^3.3.1",
		"primeicons": "^7.0.0",
		"primeng": "^17.18.1",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"winston": "^3.13.1",
		"xhr2": "^0.2.1",
		"zone.js": "~0.14.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.0.4",
		"@angular-eslint/eslint-plugin": "^18.1.0",
		"@angular-eslint/eslint-plugin-template": "^18.1.0",
		"@angular-eslint/template-parser": "^18.1.0",
		"@angular/cli": "^18.0.4",
		"@angular/compiler-cli": "^18.0.0",
		"@types/axios": "^0.14.0",
		"@types/express": "^4.17.17",
		"@types/jasmine": "~5.1.0",
		"@types/multer": "^1.4.12",
		"@types/node": "^18.18.0",
		"@typescript-eslint/eslint-plugin": "^7.15.0",
		"@typescript-eslint/parser": "^7.15.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"jasmine-core": "~5.1.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"nodemon": "^3.1.4",
		"prettier-eslint": "^16.3.0",
		"prettier-eslint-cli": "^8.0.1",
		"ts-node": "^10.9.2",
		"typescript": "~5.4.2"
	}
}
